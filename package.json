{
  "name": "this-week-admin",
  "version": "0.0.1",
  "main": "index.html",
  "scripts": {
    "start": "MODE=development parcel serve src/index.html",
    "start:all": "MODE=development parcel serve src/index.html & graphql-codegen --config codegen.ts --watch",
    "build": "MODE=production parcel build src/index.html --no-scope-hoist",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tehpsalmist/this-week-admin.git"
  },
  "keywords": [
    "parcel",
    "react",
    "tailwindcss",
    "template"
  ],
  "author": "Ben Steward <bstewardcodes@gmail.com> (https://tehpsalmist.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tehpsalmist/this-week-admin/issues"
  },
  "homepage": "https://github.com/tehpsalmist/this-week-admin#readme",
  "devDependencies": {
    "@dudadev/partner-api": "^2.0.6",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "1.2.6",
    "@parcel/transformer-svg-react": "^2.8.3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/mapbox-gl": "^2.7.17",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "buffer": "^5.7.1",
    "parcel": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "process": "^0.11.10",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@8thday/react": "^0.9.2",
    "@apollo/client": "^3.7.3",
    "@floating-ui/react": "^0.26.24",
    "@floating-ui/react-dom": "^2.1.2",
    "@formkit/tempo": "^0.0.8",
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/interaction": "^6.1.10",
    "@fullcalendar/multimonth": "^6.1.10",
    "@fullcalendar/react": "^6.1.10",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@nhost/react": "^3.6.0",
    "@nhost/react-apollo": "^13.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tiptap/extension-link": "^2.7.4",
    "@tiptap/extension-text-align": "^2.7.4",
    "@tiptap/react": "^2.7.4",
    "@tiptap/starter-kit": "^2.7.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.0",
    "clsx": "^1.2.1",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^6.6.1",
    "uuid": "^9.0.0"
